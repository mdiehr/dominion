  "scripts": {
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rm -rf dist",
    "build:dist": "webpack",
    "startl": "NODE_ENV=development webpack-dev-server --port 8000",
    "startw": "set NODE_ENV=development&& webpack-dev-server --port 8000",
    "start": "npm run startw",
    "webpack": "webpack-dev-server --port 8000",
    "lint": "eslint src/**/{*.js,*.jsx}",
    "karma": "karma start ./karma.conf.js",
    "test": "npm run karma --  --single-run --reporters nyan",
    "test:dev": "npm run karma -- --no-single-run --auto-watch --reporters nyan",
    "test:cov": "COVERAGE=1 npm run karma -- --single-run --reporters coverage",
    "check": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "babel-core": "^6.3.15",
